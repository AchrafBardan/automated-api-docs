{
    "name": "owowagency/automated-api-docs",
    "description": "Automated API docs in OWOW style.",
    "keywords": [
        "Laravel",
        "Automated",
        "API",
        "Docs"
    ],
    "authors": [
        {
            "name": "Dees Oomens",
            "email": "dees@owow.io"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/support": "5.*|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5.14",
        "orchestra/testbench": "3.5.*|3.6.*|3.7.*|4.*|5.*|6.*"
    },
    "autoload": {
       "psr-4": {
            "OwowAgency\\AutomatedApiDocs\\": "src/AutomatedApiDocs"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OwowAgency\\AutomatedApiDocs\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "OwowAgency\\AutomatedApiDocs\\ServiceProvider"
            ]
        }
    },
    "license": "MIT"
}
